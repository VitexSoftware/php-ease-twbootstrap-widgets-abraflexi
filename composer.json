{
    "name": "vitexsoftware/ease-bootstrap-widgets-abraflexi",
    "description": "php-abraflexi extending classes and widgets using twitter bootstrap3 interface",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Vitex",
            "email": "info@vitexsoftware.cz"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "vitexsoftware/ease-twbootstrap": ">=1.0",
        "vitexsoftware/abraflexi-bricks": ">=1.1"
    },
    "autoload": {
        "psr-4": {
            "AbraFlexi\\ui\\TWB\\": "src/AbraFlexi/ui/TWB/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Test\\Ease\\": "vendor/vitexsoftware/ease-framework/tests/src/Ease/",
            "Test\\AbraFlexi\\": "vendor/spoje.net/abraflexi/testing/src/AbraFlexi/",
            "Test\\AbraFlexi\\ui\\TWB\\": "testing/AbraFlexi/ui/TWB",
            "Test\\": "testing/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^12"
    }
}
